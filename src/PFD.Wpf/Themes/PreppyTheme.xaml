<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Preppy Theme (Navy, Pink, Green) -->
    <SolidColorBrush x:Key="FrameBg" Color="#1C3A5F"/>
    <SolidColorBrush x:Key="FrameBorder" Color="#0F2540"/>
    <SolidColorBrush x:Key="CalendarBg" Color="#FF85A2"/>
    <SolidColorBrush x:Key="CalendarHeaderBg" Color="#E75480"/>
    <SolidColorBrush x:Key="TaskListBg" Color="#98D4BB"/>
    <SolidColorBrush x:Key="TaskHeaderBg" Color="#2E8B57"/>
    <SolidColorBrush x:Key="TaskRowBg" Color="#C8E6D4"/>
    <SolidColorBrush x:Key="TaskRowCompletedBg" Color="#FFD1DC"/>
    <SolidColorBrush x:Key="TextPrimary" Color="#1C3A5F"/>
    <SolidColorBrush x:Key="TextSecondary" Color="#2E5A8B"/>
    <SolidColorBrush x:Key="TextLight" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="TextHeader" Color="#1C3A5F"/>
    <SolidColorBrush x:Key="InputBg" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="InputBorder" Color="#1C3A5F"/>
</ResourceDictionary>
